{
    "builds": [
      {
        "src": "django-locallibrary/locallibrary/wsgi.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.12.3" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "django-locallibrary/locallibrary/wsgi.py"
      }
    ]
}